{
  "expo": {
    "name": "Tcc",
    "slug": "Tcc",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": ".com.apple.tcc"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ], 
      [
        "expo-media-library",
        {
          "photosPermission": "Allow tcc to access your photos.",
          "savePhotosPermission": "Allow tcc to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.renantavaress.Tcc"
    }
  }
}
